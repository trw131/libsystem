/* miniprogram/pages/qiandao/qiandao.wxss */
.qiandao{
    margin: 20rpx;
    width: 90%;
    height: 100%;
    border: 1px solid rgba(0, 0, 0, 0.1);
    
}
.map{
    width: 100%;
    height: 150px;
    border: 1px solid rgba(0, 0, 0, 0.1);
    margin-top: 50rpx 
}
.circle{
    margin:100px auto;
    background-color: aqua;
    width: 300rpx;
    height: 300rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.circle2{
    background-color: white;
    width: 280rpx;
    height: 280rpx;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.time,.textqiandao{
    text-align: center;
    font-size: 18px;
    color: aquamarine;
    font-weight: 700;
    display: block;
}
.textqiandao{
    text-align: center;

}